{
  "name": "Netflix Instant Arrow",
  "short_name": "Netflix Instant Arrow",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Reduce forward and back skip action '>(or <) + space' to '>(or <)'",
  "permissions": ["*://*.netflix.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["js/watch.js"]
    }
  ]
}
